
<!-- Modal -->
<div class="modal fade" id="warningLogoutModal" tabindex="-1" role="dialog" aria-labelledby="warningModalTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="warningModalTitle">Idle Warning</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="cancelTimeOut()">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <h1 class="text-center"><em class="bi bi-x-circle-fill"></em></h1>
                <p class="text-center">Hey! you will be logged out if you don't do something!</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="cancelTimeOut()">Close</button>
                @*<button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div>
    </div>
</div>